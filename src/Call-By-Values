#include<iostream.h>
#include<conio.h>
void swap(int num1,int num2)
{	int temp;
	temp=num1;
	num1=num2;
	num2=temp;
	cout<<"\nAfter swapping\na:"<<num1<<" "<<"b:"<<num2;
}
void main()
{	clrscr();
	int a,b;
	cout<<"\nEnter two nos. :";
	cin>>a>>b;
	cout<<"\nBefore swapping\na:"<<a<<" "<<"b:"<<b;
	swap(a,b);
	getch();
}
